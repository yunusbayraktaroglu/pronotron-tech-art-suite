{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"inputs": [
				"$TURBO_DEFAULT",
				".env*"
			],
			"outputs": [
				"dist/**",
				".next/**",
				"!.next/cache/**"
			],
			"dependsOn": [
				"^build"
			]
		},
		"docs": {},
		"test": {
			"outputs": [
				"coverage/**"
			],
			"dependsOn": []
		},
		"test:watch": {
    		"cache": false,
    		"persistent": true
    	},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		}
	}
}